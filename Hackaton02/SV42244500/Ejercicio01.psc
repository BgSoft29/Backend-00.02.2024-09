//determinar si tiene tres dígitos.
Proceso Ejercicio01
	
	Escribir "Escribir un numero por el teclado"
	Leer numero
	
	si numero > 99 y numero < 1000 Entonces
		Escribir "El numero tiene 3 digitos"
	SiNo
		Escribir "El numero no tiene 3 digitos"
	FinSi
	
FinProceso
